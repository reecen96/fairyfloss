
<%= form_with model: @application, local: true, html: { multipart: true} do |f| %>

<%= f.label :title %>
<%= f.text_field :title %>
<br>
<%= f.label :message %>
<%= f.text_field :message, :class => "text" %>
<br>
<%= f.label :offer %>
<%= f.text_field :offer %>
<br>
<%= f.label :image %>
<%= f.file_field :image %>


<br>
<%= f.hidden_field :house_num, :value => 32%>
<%= f.submit "Apply"%>



<% end %>
